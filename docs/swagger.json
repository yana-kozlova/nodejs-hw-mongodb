                  "password"
                ],
                "properties": {
                  "email": {
                    "description": "User email for login.",
                    "type": "string",
                    "format": "email",
                    "example": "yana@example.com"
                  },
                  "password": {
                    "description": "User password.",
                    "type": "string",
                    "format": "password",
                    "example": "securePass123"
                  }
                }
              }
